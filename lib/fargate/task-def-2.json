{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "containerDefinitions": [
        {
            "name": "wiz-1-container",
            "image": "657781215424.dkr.ecr.us-east-1.amazonaws.com/conjure/watched-1:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": "3000",
                    "protocol": "tcp"
                },
                {
                    "containerPort": "80",
                    "protocol": "tcp"
                }
            ],
            "entryPoint": [],
            "command": [
                "node server.js"
            ],
            "environment": null,
            "mountPoints": null,
            "volumesFrom": null,
            "hostname": null,
            "user": null,
            "workingDirectory": null,
            "extraHosts": null,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/wiz-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "ulimits": null,
            "dockerLabels": null
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::657781215424:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::657781215424:role/ecsTaskExecutionRole",
    "family": "wiz-task"
}